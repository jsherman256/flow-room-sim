application buoyantBoussinesqSimpleFoam;
startFrom startTime;
stopAt endTime;
purgeWrite 0;
writeFormat ascii;
writePrecision 12;
writeCompression on;
timeFormat general;
timePrecision 12;
runTimeModifiable yes;
deltaT 1.0;
startTime 0;
endTime 1000.0;
adjustTimeStep no;
writeControl timeStep;
writeInterval 1000;
libs ("libchtgfm.so" "libsimScaleFunctionObjects.so");
